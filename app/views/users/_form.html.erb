
<%= form_with(model: user, local: true) do |f| %>
  
  <div class="form-inputs">
  </div>

  <div class="actions">
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    

    <%= f.submit %>
  </div>
<% end %>
